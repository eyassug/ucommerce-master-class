<configuration>
	<components>
		<component
			id="TaxService"
			service="UCommerce.Catalog.ITaxService, UCommerce"
			type="MyUCommerceApp.BusinessLogic.Pricing.ExtendedTaxService, MyUCommerceApp.BusinessLogic" />

		<component
			id="QuantityShippingMethodService"
