			type="MyUCommerceApp.BusinessLogic.Shipping.QuantityShippingMethodService, MyUCommerceApp.BusinessLogic">
			<parameters>
				<multiplier>10</multiplier>
				<upperPriceLimit>100</upperPriceLimit>
			</parameters>
		</component>
		
	</components>
</configuration>